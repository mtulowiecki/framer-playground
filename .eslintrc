{
  "extends": [
    "react-app",
    "airbnb",
    "airbnb/hooks",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": ["jsx-a11y", "prettier"],
  "globals": {
    "document": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "object-curly-newline": ["warning", { "multiline": true }]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": ".js",
        "paths": ["src"]
      }
    }
  }
}
